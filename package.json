{
  "name": "southore",
  "private": false,
  "version": "0.0.1",
  "description": "Accessible React component library built on React Aria Components",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/perkola/southore.git"
  },
  "homepage": "https://github.com/perkola/southore",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "test": "vitest run --project components && vitest run --project components-dark --testNamePattern '^screenshot:'",
    "test:watch": "vitest --project components --project components-dark --testNamePattern '^screenshot:'",
    "test:coverage": "vitest run --project components --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "lucide-react": "^0.563.0",
    "react-aria-components": "^1.15.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@storybook/addon-a11y": "^10.2.6",
    "@storybook/addon-docs": "^10.2.6",
    "@storybook/addon-mcp": "^0.2.2",
    "@storybook/addon-vitest": "^10.2.6",
    "@storybook/react-vite": "^10.2.6",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.2.6",
    "globals": "^16.5.0",
    "playwright": "^1.58.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "^10.2.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@latest",
    "vitest": "^4.0.18",
    "vitest-browser-react": "^2.0.5"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@latest"
  }
}
